
import javax.swing.*;
import java.util.*;

public class TestStatus {
	
	public static void main(String[] args) {
		for (int index=0; index < status.length; index++) {
		     status[index].displayStatus();
		}
		
		String out = “”;

		for (int index=0; index < status.length; index++) {
			out = out + status[index].getStatus();
		}

		JOptionPane.showMessageDialog(null, out);
		System.exit(0);
	}
	
}
