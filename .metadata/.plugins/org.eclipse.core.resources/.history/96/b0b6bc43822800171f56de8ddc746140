
public class GuessNotWholeNumberException extends Exception {

	public GuessNotWholeNumberException() {
		// TODO Auto-generated constructor stub
	}

	public GuessNotWholeNumberException(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	public GuessNotWholeNumberException(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}

	public GuessNotWholeNumberException(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	public GuessNotWholeNumberException(String message, Throwable cause, boolean enableSuppression,
			boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
		// TODO Auto-generated constructor stub
	}

}
